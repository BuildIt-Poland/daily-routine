  "private": true,
  "homepage": ".",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "cypress": "^3.3.1",
    "gh-pages": "^2.0.1",
    "serve": "^11.0.1",
    "serve-handler": "^6.0.1"
  },
  "scripts": {
